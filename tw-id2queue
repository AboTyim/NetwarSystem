#!/usr/bin/python
# twitter account dispatcher
import csv, json, os, random, re, sys, time, hashlib
import redis
from walrus import *

wal = Walrus(host='localhost', port=6379, db=0)
q0 = wal.Set("queue0work")
q1 = wal.Set("queue1work")
q2 = wal.Set("queue2work")
q3 = wal.Set("queue3work")
q4 = wal.Set("queue4work")
q5 = wal.Set("queue5work")
q6 = wal.Set("queue6work")
q7 = wal.Set("queue7work")

accts = open(sys.argv[1], 'r')

for acct in accts:
	acct = acct.rstrip()
	qq = hashlib.md5(acct).hexdigest()[5]
	print(qq + " " + acct)
        if(qq == "0"):
		q0.add(acct)
        if(qq == "1"):
		q0.add(acct)
        if(qq == "2"):
		q1.add(acct)
        if(qq == "3"):
		q1.add(acct)
        if(qq == "4"):
		q2.add(acct)
        if(qq == "5"):
		q2.add(acct)
        if(qq == "6"):
		q3.add(acct)
        if(qq == "7"):
		q3.add(acct)
        if(qq == "8"):
		q4.add(acct)
        if(qq == "9"):
		q5.add(acct)
        if(qq == "a"):
		q5.add(acct)
        if(qq == "b"):
		q5.add(acct)
        if(qq == "c"):
		q6.add(acct)
        if(qq == "d"):
		q6.add(acct)
        if(qq == "e"):
		q7.add(acct)
        if(qq == "f"):
		q7.add(acct)
